   "source": [
    "xs = np.linspace(0, 100, 1000)\n",
    "# sine wave\n",
    "ys = np.sin(xs) * 20\n",
    "\n",
    "# make 25 lines\n",
    "data = np.vstack([ys] * 25)\n",
    "\n",
    "plot = fpl.Plot()\n",
    "\n",
    "plot.add_line_stack(data, cmap=\"jet\")\n",
    "\n",
    "plot.show(maintain_aspect=False)"
   ]
